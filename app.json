{
  "expo": {
    "name": "Stockline",
    "slug": "Stockline",
    "scheme": "Stockline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/union.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashs.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.jacceycode.Stockline"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/union.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jacceycode.Stockline"
    },
    "web": {
      "favicon": "./assets/union.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3cd4c951-a38c-4278-b251-98cd1131251f"
      }
    }
  }
}
